{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Create Makefile",
			"type": "process",
			"command": "cscript.exe",
			"args": [
				"//nologo",
				"C:\\Programming\\FreeBASIC Projects\\ProgrammingStyle\\CreateMakefile\\CreateMakefile.vbs",
				"/out:Station922",
				"/unicode:true",
				"/wrt:true",
				"/addressaware:true",
